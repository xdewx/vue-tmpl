{
  "name": "@leoking/vue-tmpl",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "lib",
    "eslint.config.js",
    "auto-imports.d.ts",
    "components.d.ts",
    "tsconfig.lib.json"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./lib/index.es.js",
        "types": "./lib/index.d.ts"
      },
      "require": {
        "default": "./lib/index.cjs",
        "types": "./lib/index.d.ts"
      }
    },
    "./index.css": "./lib/index.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:lib": "vite build --config vite.config.lib.ts && vue-tsc -p tsconfig.lib.json",
    "preview": "vite preview",
    "lint": "eslint .",
    "prettier:format": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test": "vitest"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^5.3.1",
    "@vueuse/core": "^11.2.0",
    "@vueuse/head": "^2.0.0",
    "@vueuse/math": "^11.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "lint-staged": "^15.2.10",
    "pinia": "^2.2.6",
    "prettier": "^3.5.3",
    "simple-git-hooks": "^2.11.1",
    "vue": "^3.5.12",
    "vue-i18n": "10",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/bun": "latest",
    "@unocss/eslint-config": "^0.64.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue-macros/eslint-config": "^0.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0",
    "unocss": "^0.64.0",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-icons": "^0.20.1",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-router": "^0.10.9",
    "vite": "^5.4.10",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "bun run lint",
    "preserveUnused": true
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
