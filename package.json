{
  "name": "@leoking/vue-tmpl",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "lib",
    "eslint.config.js",
    "auto-imports.d.ts",
    "components.d.ts",
    "tsconfig.lib.json"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./lib/index.es.js",
        "types": "./lib/index.d.ts"
      },
      "require": {
        "default": "./lib/index.cjs",
        "types": "./lib/index.d.ts"
      }
    },
    "./index.css": "./lib/index.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "build:lib": "vite build --config vite.config.lib.ts && vue-tsc -p tsconfig.lib.json",
    "preview": "vite preview",
    "lint": "eslint src",
    "format": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test": "vitest"
  },
  "dependencies": {
    "@intlify/unplugin-vue-i18n": "^5.3.1",
    "@vueuse/core": "^13.3.0",
    "@vueuse/head": "^2.0.0",
    "@vueuse/math": "^13.3.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "lint-staged": "^16.1.0",
    "pinia": "^3.0.2",
    "prettier": "^3.5.3",
    "vue": "^3.5.16",
    "vue-i18n": "^10.0.7",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/bun": "latest",
    "@unocss/eslint-config": "^66.1.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue-macros/eslint-config": "^0.7.1",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.1.0",
    "globals": "^16.2.0",
    "simple-git-hooks": "^2.13.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.1",
    "unocss": "^0.64.1",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-icons": "^0.20.2",
    "unplugin-vue-components": "^0.27.5",
    "unplugin-vue-router": "^0.10.9",
    "vite": "^5.4.19",
    "vitest": "^2.1.9",
    "vue-tsc": "^2.2.10"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "bun run lint",
    "preserveUnused": true
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}